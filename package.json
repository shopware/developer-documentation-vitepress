{
  "name": "vitepress-shopware-docs",
  "version": "1.1.36",
  "description": "Vitepress theme for Shopware",
  "main": "src/index.ts",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/shopware/config/baseConfigLegacy.js",
    "./highlight": "./src/shopware/config/highlight.js",
    "./src/*.css": {
      "import": "./src/*.css",
      "require": "./src/*.css"
    },
    "./src/*.scss": {
      "import": "./src/*.scss",
      "require": "./src/*.scss"
    }
  },
  "files": [
    "src",
    "types"
  ],
  "keywords": [
    "vue",
    "shopware",
    "vitepress"
  ],
  "author": {
    "name": "Shopware team"
  },
  "license": "MIT",
  "scripts": {
    "lint:full": "pnpm run lint && pnpm run type",
    "lint": "prettier --check --write --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "type": "tsc --noEmit",
    "test:unit": "vitest run -r __tests__/unit --sequence.shuffle",
    "test:cli": "vitest run -r __tests__/cli --sequence.shuffle",
    "test:cli:coverage": "vitest run -r __tests__/cli --coverage",
    "dev": "vitepress dev demo",
    "build": "vitepress build demo",
    "serve": "pnpm run build && vitepress serve demo --port 5002",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --exit-zero-on-changes",
    "bump": "npx lerna version --no-private --no-push",
    "cli:dev": "pnpm -C ./packages/cli dev && chmod +x ./packages/cli/dist/docs-cli.js",
    "cli:build": "pnpm -C ./packages/cli build && chmod +x ./packages/cli/dist/docs-cli.js",
    "cli:pkg": "pnpm -C ./packages/cli pkg"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.34",
    "@shopware-ag/meteor-icon-kit": "^5.3.0",
    "@stoplight/elements-dev-portal": "^2.1.6",
    "@stoplight/json-schema-generator": "^1.0.2",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.7.2",
    "body-scroll-lock": "^3.1.5",
    "fs-extra": "^11.2.0",
    "fsevents": "^2.3.3",
    "gray-matter": "^4.0.3",
    "medium-zoom": "^1.1.0",
    "normalize.css": "^8.0.1",
    "semver": "^7.6.2",
    "shiki": "^1.6.0",
    "unocss": "^0.60.3"
  },
  "peerDependencies": {
    "@docsearch/css": "^3.5.2",
    "@docsearch/js": "^3.5.2",
    "algoliasearch": "^4.20.0",
    "instantsearch.css": "^8.1.0",
    "instantsearch.js": "^4.61.0",
    "vue-instantsearch": "^4.13.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^8.1.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^8.1.3",
    "@storybook/vue3-vite": "^8.1.3",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/estree": "^1.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.12.12",
    "@types/remove-markdown": "^0.3.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vueuse/shared": "10.9.0",
    "chromatic": "^11.4.0",
    "esbuild": "^0.21.3",
    "get-port": "^7.1.0",
    "google-fonts-helper": "^3.6.0",
    "lerna": "^8.1.3",
    "lorem-ipsum": "^2.0.8",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remove-markdown": "^0.5.0",
    "sass": "^1.77.2",
    "seedrandom": "^3.0.5",
    "storybook": "^8.1.3",
    "storybook-addon-fetch-mock": "^2.0.0",
    "storybook-addon-pseudo-states": "^3.1.1",
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitepress": "1.2.2",
    "vitest": "^1.6.0",
    "vue": "^3.4.27"
  },
  "packageManager": "pnpm@8.11.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "vite",
        "@types/react",
        "mkdirp",
        "@stoplight/mosaic-code-viewer"
      ]
    }
  }
}
