{
  "name": "vitepress-shopware-docs",
  "version": "0.3.0-beta.10",
  "description": "Vitepress theme for Shopware",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/vitepress/config/baseConfig.js",
    "./highlight": "./src/vitepress/config/highlight.js"
  },
  "files": [
    "src",
    "types"
  ],
  "keywords": [
    "vue",
    "shopware",
    "vitepress"
  ],
  "author": "Shopware team",
  "license": "MIT",
  "scripts": {
    "lint": "prettier --check --write --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "type": "tsc --noEmit",
    "test": "pnpm run lint && pnpm run type",
    "dev": "vitepress dev demo",
    "build": "vitepress build demo",
    "serve": "pnpm run build && vitepress serve demo --port 5002"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.2",
    "@docsearch/js": "^3.3.2",
    "@iconify-json/carbon": "^1.1.14",
    "@stoplight/elements-dev-portal": "^1.9.5",
    "@vueuse/core": "^9.11.1",
    "axios": "^0.27.2",
    "body-scroll-lock": "^3.1.5",
    "normalize.css": "^8.0.1",
    "shiki": "^0.12.1",
    "unocss": "^0.49.0",
    "vitepress-plugin-mermaid": "^2.0.8",
    "mermaid": "9.1.7",
    "fs-extra": "^11.1.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "^3.1.0",
    "@types/estree": "^0.0.52",
    "@types/fs-extra": "^11.0.1",
    "@types/remove-markdown": "^0.3.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.18",
    "prettier": "^2.8.3",
    "remove-markdown": "^0.5.0",
    "terser": "^5.16.1",
    "typescript": "^4.9.4",
    "vitepress": "1.0.0-alpha.40",
    "vue": "^3.2.45",
    "google-fonts-helper": "^3.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "vite",
        "@types/react",
        "mkdirp",
        "@stoplight/mosaic-code-viewer"
      ]
    }
  }
}
