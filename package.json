{
  "name": "vitepress-shopware-docs",
  "version": "1.0.12",
  "description": "Vitepress theme for Shopware",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/shopware/config/baseConfig.js",
    "./highlight": "./src/shopware/config/highlight.js",
    "./src/*.css": {
      "import": "./src/*.css",
      "require": "./src/*.css"
    },
    "./src/*.scss": {
      "import": "./src/*.scss",
      "require": "./src/*.scss"
    }
  },
  "files": [
    "src",
    "types"
  ],
  "keywords": [
    "vue",
    "shopware",
    "vitepress"
  ],
  "author": {
    "name": "Shopware team"
  },
  "license": "MIT",
  "scripts": {
    "lint:full": "pnpm run lint && pnpm run type",
    "lint": "prettier --check --write --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "type": "tsc --noEmit",
    "test:unit": "vitest run -r __tests__/unit --sequence.shuffle",
    "test:cli": "vitest run -r __tests__/cli --sequence.shuffle",
    "test:cli:coverage": "vitest run -r __tests__/cli --coverage",
    "dev": "vitepress dev demo",
    "build": "vitepress build demo",
    "serve": "pnpm run build && vitepress serve demo --port 5002",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --exit-zero-on-changes",
    "bump": "npx lerna version --no-private --no-push",
    "cli:dev": "pnpm -C ./packages/cli dev && chmod +x ./packages/cli/dist/docs-cli.js",
    "cli:build": "pnpm -C ./packages/cli build && chmod +x ./packages/cli/dist/docs-cli.js",
    "cli:pkg": "pnpm -C ./packages/cli pkg"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.21",
    "@shopware-ag/meteor-icon-kit": "^5.2.0",
    "@stoplight/elements-dev-portal": "^1.16.8",
    "@stoplight/json-schema-generator": "^1.0.2",
    "@vueuse/core": "^10.5.0",
    "axios": "^1.5.1",
    "body-scroll-lock": "^3.1.5",
    "fs-extra": "^11.1.1",
    "fsevents": "^2.3.3",
    "gray-matter": "^4.0.3",
    "medium-zoom": "^1.0.8",
    "normalize.css": "^8.0.1",
    "shiki": "^0.14.5",
    "unocss": "^0.57.1"
  },
  "peerDependencies": {
    "@docsearch/css": "^3.5.2",
    "@docsearch/js": "^3.5.2",
    "algoliasearch": "^4.20.0",
    "instantsearch.css": "^8.1.0",
    "instantsearch.js": "^4.58.0",
    "vue-instantsearch": "^4.10.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.5.1",
    "@storybook/vue3-vite": "^7.5.1",
    "@types/body-scroll-lock": "^3.1.1",
    "@types/estree": "^1.0.3",
    "@types/fs-extra": "^11.0.3",
    "@types/markdown-it": "^13.0.5",
    "@types/node": "^18.18.6",
    "@types/remove-markdown": "^0.3.3",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vueuse/shared": "10.5.0",
    "chromatic": "^6.24.1",
    "get-port": "^6.1.2",
    "google-fonts-helper": "^3.3.1",
    "lerna": "^6.6.2",
    "lorem-ipsum": "^2.0.8",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remove-markdown": "^0.5.0",
    "sass": "^1.69.4",
    "seedrandom": "^3.0.5",
    "storybook": "^7.5.1",
    "storybook-addon-fetch-mock": "^1.0.1",
    "storybook-addon-pseudo-states": "^2.1.2",
    "terser": "^5.22.0",
    "typescript": "^5.2.2",
    "vitepress": "1.0.0-rc.24",
    "vitest": "^0.34.6",
    "vue": "^3.3.6",
    "esbuild": "^0.18.20"
  },
  "packageManager": "pnpm@8.6.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "vite",
        "@types/react",
        "mkdirp",
        "@stoplight/mosaic-code-viewer"
      ]
    },
    "overrides": {
      "@antfu/utils@<0.7.3": ">=0.7.3",
      "vite@>=4.3.0 <4.3.9": ">=4.3.9",
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "get-func-name@<2.0.1": ">=2.0.1",
      "postcss@<8.4.31": ">=8.4.31",
      "@babel/traverse@<7.23.2": ">=7.23.2"
    }
  }
}
