{
  "name": "vitepress-shopware-docs",
  "version": "1.0.0-alpha.12",
  "description": "Vitepress theme for Shopware",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/shopware/config/baseConfig.js",
    "./highlight": "./src/shopware/config/highlight.js"
  },
  "files": [
    "src",
    "types"
  ],
  "keywords": [
    "vue",
    "shopware",
    "vitepress"
  ],
  "author": {
    "name": "Shopware team"
  },
  "license": "MIT",
  "scripts": {
    "lint": "prettier --check --write --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "type": "tsc --noEmit",
    "test": "pnpm run lint && pnpm run type",
    "test:unit": "vitest run -r __tests__/unit --sequence.shuffle",
    "dev": "vitepress dev demo",
    "build": "vitepress build demo",
    "serve": "pnpm run build && vitepress serve demo --port 5002",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.3",
    "@docsearch/js": "^3.3.3",
    "@iconify-json/carbon": "^1.1.16",
    "@shopware-ag/meteor-icon-kit": "^5.1.0",
    "@stoplight/elements-dev-portal": "^1.9.11",
    "@stoplight/json-schema-generator": "^1.0.2",
    "@vueuse/core": "^9.13.0",
    "axios": "^0.27.2",
    "body-scroll-lock": "^3.1.5",
    "fs-extra": "^11.1.1",
    "fsevents": "^2.3.2",
    "gray-matter": "^4.0.3",
    "normalize.css": "^8.0.1",
    "shiki": "^0.12.1",
    "unocss": "^0.49.8"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-styling": "^1.0.2",
    "@storybook/blocks": "^7.0.7",
    "@storybook/testing-library": "0.0.14-next.2",
    "@storybook/vue3": "^7.0.7",
    "@storybook/vue3-vite": "^7.0.7",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/estree": "^0.0.52",
    "@types/fs-extra": "^11.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.16.0",
    "@types/remove-markdown": "^0.3.1",
    "@vitejs/plugin-vue": "^4.2.0",
    "chromatic": "^6.17.3",
    "get-port": "^6.1.2",
    "google-fonts-helper": "^3.3.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remove-markdown": "^0.5.0",
    "sass": "^1.62.0",
    "storybook": "^7.0.7",
    "storybook-addon-fetch-mock": "^1.0.1",
    "terser": "^5.17.1",
    "typescript": "^4.9.5",
    "vitepress": "1.0.0-alpha.75",
    "vitest": "^0.28.5",
    "vue": "^3.2.47"
  },
  "packageManager": "pnpm@8.1.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "vite",
        "@types/react",
        "mkdirp",
        "@stoplight/mosaic-code-viewer"
      ]
    }
  }
}
